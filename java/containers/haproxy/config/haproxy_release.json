{
  "serviceName": "LoadBalancer",
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": false,
  "isPrivileged": true,
  "useHostNetwork": true,
  "containerImage": "esxcloud/haproxy",
  "containerName": "haproxy",
  "portBindings": {
    "8080": 8080,
    "443": 443,
    "80": 80,
    "4343": 4343
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "LOG_DIRECTORY": "/var/log",
    "LOAD_BALANCER_HTTPS_PORT" : 443,
    "MANAGEMENT_UI_HTTP_PORT" : 80,
    "MANAGEMENT_UI_HTTPS_PORT" : 4343
  }
}
