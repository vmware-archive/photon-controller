{
  "serviceName": "Chariman",
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": true,
  "isPrivileged": false,
  "useHostNetwork": true,
  "containerImage": "esxcloud/chairman",
  "portBindings": {
    "13000": 13000
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "LOG_DIRECTORY": "/var/log/esxcloud",
    "CHAIRMAN_INSTALL_DIRECTORY": "/usr/lib/esxcloud/chairman"
  }
}
