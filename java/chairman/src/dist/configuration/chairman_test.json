{
  "serviceName": "Chariman",
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": true,
  "isPrivileged": false,
  "containerImage": "photon/chairman",
  "containerName": "chairman",
  "portBindings": {
    "13000": 13000
  },
  "volumeBindings": {
    "/var/log": "/var/log",
    "/vagrant/log/chairman": "/vagrant/log",
    "/devbox_data/tmp": "/devbox_data/tmp",
    "/var/esxcloud": "/var/esxcloud"
  },
  "dynamicParameters": {
    "VM_IP": "localhost",
    "ZOOKEEPER_QUORUM": "localhost:2181",
    "LOG_DIRECTORY": "/vagrant/log",
    "CHAIRMAN_INSTALL_DIRECTORY": "/usr/lib/esxcloud/chairman",
    "DEPLOYMENT_ID": "test-deployment",
    "JAVA_DEBUG": "-agentlib:jdwp=transport=dt_socket,server=y,address=23000,suspend=n"
  }
}
