{
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 8,
  "isReplicated": true,
  "isPrivileged": false,
  "containerImage": "esxcloud/housekeeper",
  "portBindings": {
    "16000": 16000,
    "16001": 16001
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "HOUSEKEEPER_BIND_ADDRESS": "0.0.0.0",
    "HOUSEKEEPER_DCP_STORAGE_PATH": "/etc/esxcloud/housekeeper/sandbox_16000",
    "LOG_DIRECTORY": "/var/log/esxcloud",
    "HOUSEKEEPER_PORT": 16000,
    "HOUSEKEEPER_INSTALL_DIRECTORY": "/usr/lib/esxcloud/housekeeper"
  }
}
