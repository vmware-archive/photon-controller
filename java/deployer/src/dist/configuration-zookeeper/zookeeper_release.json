{
  "serviceName": "Zookeeper",
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": true,
  "isPrivileged": false,
  "useHostNetwork": true,
  "containerImage": "esxcloud/zookeeper",
  "portBindings": {
    "2181": 2181,
    "2888": 2888,
    "3888": 3888
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "ZOOKEEPER_DATA_DIRECTORY": "/var/esxcloud/data/zookeeper"
  }
}
