{
  "serviceName": "PhotonControllerCore",
  "cpuCount": 4,
  "memoryMb": 8192,
  "diskGb": 27,
  "isReplicated": true,
  "isPrivileged": true,
  "containerImage": "photon/photon-controller-core",
  "containerName": "photon-controller-core",
  "portBindings": {
    "9000": 9000,
    "19000": 19000
  },
  "volumeBindings": {
    "/devbox_data/tmp": "/devbox_data/tmp",
    "/vagrant/log": "/vagrant/log",
    "/var/log": "/var/log",
    "/var/photon/images": "/var/photon/images",
    "/sys/fs/cgroup": "/sys/fs/cgroup"
  },
  "dynamicParameters": {
    "CLOUD-STORE_BIND_ADDRESS": "0.0.0.0",
    "CLOUD-STORE_XENON_STORAGE_PATH": "/etc/esxcloud/cloud-store/sandbox_19000",
    "CLOUD-STORE_INSTALL_DIRECTORY": "/usr/lib/esxcloud/cloud-store",
    "CLOUD-STORE_PORT": 19000,
    "JAVA_DEBUG": "-agentlib:jdwp=transport=dt_socket,server=y,address=29000,suspend=n",
    "LOG_DIRECTORY": "/vagrant/log",
    "MANAGEMENT-API_INSTALL_DIRECTORY": "/usr/lib/esxcloud/management-api",
    "USE_ESX_STORE": true,
    "PHOTON-CONTROLLER-CORE_INSTALL_DIRECTORY": "/usr/lib/esxcloud/photon-controller-core",
    "APIFE_PORT": 9000,
    "TENANT_NAME": "mgmt-tenant",
    "PROJECT_NAME": "mgmt-project",
    "RESOURCE_TICKET_NAME": "mgmt-res-ticket",
    "MAX_MEMORY_GB": 64,
    "MAX_VM_COUNT": 20,
    "VIB_DIRECTORY": "/var/esxcloud/packages"
  }
}
