{
  "cpuCount": 1,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": true,
  "isPrivileged": false,
  "containerImage": "esxcloud/rootscheduler",
  "portBindings": {
    "13010": 13010
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "ROOT-SCHEDULER_BIND_ADDRESS": "0.0.0.0",
    "ROOT-SCHEDULER_PORT": 13010,
    "LOG_DIRECTORY": "/var/log/esxcloud",
    "ROOT-SCHEDULER_INSTALL_DIRECTORY": "/usr/lib/esxcloud/root-scheduler"
  }
}
