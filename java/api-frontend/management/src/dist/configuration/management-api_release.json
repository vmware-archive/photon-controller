{
  "serviceName": "ManagementApi",
  "cpuCount": 2,
  "memoryMb": 1024,
  "diskGb": 1,
  "isReplicated": true,
  "isPrivileged": false,
  "useHostNetwork": true,
  "containerImage": "esxcloud/management-api",
  "portBindings": {
    "9000": 9000
  },
  "volumeBindings": {
    "/var/log": "/var/log"
  },
  "dynamicParameters": {
    "LOG_DIRECTORY": "/var/log/esxcloud",
    "MANAGEMENT-API_INSTALL_DIRECTORY": "/usr/lib/esxcloud/management-api",
    "USE_ESX_STORE": true,
    "USE_VIRTUAL_NETWORK": false
  }
}
