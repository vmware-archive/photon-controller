{
  "serviceName": "Lightwave",
  "cpuCount": 1,
  "memoryMb": 4096,
  "diskGb": 8,
  "isReplicated": false,
  "isPrivileged": true,
  "useHostNetwork": true,
  "containerImage": "vmware/lightwave-sts:1.0.2",
  "containerName": "lightwave",
  "portBindings": {
    "443":443
  },
  "volumeBindings": {
    "/devbox_data/controller/containers/lightwave/build/config": "/etc/lightwave",
    "/vagrant/log/lightwave": "/var/log",
    "/devbox_data/controller/containers/lightwave/build/config": "/var/lib/vmware/config",
    "/sys/fs/cgroup":"/sys/fs/cgroup"
  },
  "dynamicParameters": {
    "LOG_DIRECTORY": "/vagrant/log"
  }
}
